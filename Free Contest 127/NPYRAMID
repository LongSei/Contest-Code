#include<bits/stdc++.h>
using namespace std;
#define int long long 
#define faster ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
const int MAX = 205;

void solve() {
	int n, v;
	cin >> n >> v;
	cout << v << endl;
	for (int i = 2; i <= n; i++) {
		for (int j = 1; j <= i - 1; j++) {
			cout << 0 << " ";
		}
		cout << v << endl;
	}
}

signed main() {
	faster;
	int test; cin >> test;
	while (test--) {
		solve();
	}
}
